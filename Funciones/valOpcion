#Funcion valOpcion
.data
        .globl main
		.text

# La etiqueta main representa el punto de partida		
 main:
 	addi $sp,$sp,-4
 	sw $ra,0($sp)
 	
  	addi $s0,$zero,3  #Dato provicional
	addi $t0,$zero,11
	addi $t1,$zero,1
	
	slt $t0,$s0,$t0   
	slt $t1,$s0,$t1  
	
	beq $t0,$0,exit
	beq $t1,1, exit
	li $v0, 0
	syscall
	exit: 	
		li $v0,1
		syscall
