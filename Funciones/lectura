# Leer un archivo. 

        .data
fout:   .asciiz "C:\\Users\\Francisco\\Desktop\\Ensamblador\\pokeTypes.txt"      # ruta Absoluta..
buffer: .space 1024
        .text

main:

  la $a0, fout
  jal read
  
  la $a0, ($v0)
  li $v0, 4
  syscall
  
  li $v0, 10
  syscall

read:
#Aqui lo abre
  li $v0, 13       # Aqui se abre el archivo
  li $a1, 0        # bandera
  syscall          # abre el archivo... fd en v0

  move $t1, $v0    	
  
 #Aqui lo lee
  li $v0, 14       # instruccion para leer en v0
  la $a1, buffer   # direccionamiento del buffer
  li $a2, 1024    
  move $a0, $t1    # direcciona el fd en a0		
  syscall          


#Aqui lo cierra
  li $v0, 16       # intruccion para cerrar
  move $a0, $t1   # redirrecciona el fd a a0
  syscall         

  la $v0, buffer #carga en v0
  jr $ra